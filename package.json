{
  "name": "Dani'sContactsApp",
  "version": "1.0.0",
  "description": "Build a contacts manager web application with Express and PostgreSQL.",
  "scripts": {
    "start": "node server.js",
    "start:dev": "nodemon server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "db:build": "npm run db:create && npm run db:setup && npm run db:seed",
    "db:reset": "npm run db:drop && db:create",
      "db:create": "createdb contacts",
      "db:setup": "psql contacts< ./schema.sql",
      "db:seed": "psql contacts< ./contacts.sql",
      "db:drop": "dropdb contacts"
  },
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.17.2",
    "ejs": "^2.5.6",
    "express": "^4.15.3",
    "morgan": "^1.8.2",
    "pg": "^6.2.3"
  },
  "devDependencies": {}
}
